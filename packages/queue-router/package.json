{
  "name": "@ask-me/queue-router",
  "version": "0.1.0",
  "description": "A router for Cloudflare Queue events",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@vitest/coverage-v8": "^3.2.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.2"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": "^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0"
}
