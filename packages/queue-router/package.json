{
  "name": "@ask-me/queue-router",
  "version": "0.1.0",
  "description": "A router for Cloudflare Queue events",
  "type": "module",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250607.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "~3.1.4"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": "^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0"
}
